{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "Bundler",
    "lib": ["esnext"],
    "strict": true,
    "types": ["@cloudflare/workers-types/2022-03-21"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "esnext",
    "allowJs": true,
    "moduleDetection": "force",
    "isolatedModules": true
  },
  "exclude": ["node_modules", "dist", "test"],
  "include": ["src"]
}
